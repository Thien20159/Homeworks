height = int(input('Height?: '))
width = int(input('Width?: '))

asterisk = '*'
for i in range(1,height + 1):
    space = ''
    for j in range(1,width + 1):
        if i == 1 or i == height:
            space += asterisk
        else:
            if j == 1 or j == width:
                space += asterisk
            else:
                space += ' '
    print(space)
